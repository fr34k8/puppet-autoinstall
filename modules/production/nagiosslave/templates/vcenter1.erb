<%= server_to_mon %>,Cluster Memory Usage,check_vmware.pl --server <%= vcenter_server %> --config /etc/viconfig1 --report=memory
<%= server_to_mon %>,Cluster Cpu Usage,check_vmware.pl --server <%= vcenter_server %> --config /etc/viconfig1 --report=cpu 
<%= server_to_mon %>,Cluster Network Usage,check_vmware.pl --server <%= vcenter_server %> --config /etc/viconfig1 --report=net
<%= server_to_mon %>,Cluster Disk Usage,check_vmware.pl --server <%= vcenter_server %> --config /etc/viconfig1 --report=disk
<%= server_to_mon %>,C Partition,check_snmp_storage.pl -H <%= server_to_mon %> -2 -C public -T pu -w 90% -c 95% -m C: -f
<%= server_to_mon %>,D Partition,check_snmp_storage.pl -H <%= server_to_mon %> -2 -C public -T pu -w 95% -c 99% -m D: -f
<%= server_to_mon %>,Current load,check_snmp_load.pl -H <%= server_to_mon %> -C public -2 -w 90 -c 95 -f
<%= server_to_mon %>,Memory Usage,check_snmp_storage.pl -H <%= server_to_mon %> -2 -C public -T pu -w 99 -c 100  -m  -f -q Ram
<%= server_to_mon %>,Parent slave,check_myslave.sh
